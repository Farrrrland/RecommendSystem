\n      width=\"30%\">\n      <span>确认注销吗？</span>\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"dialogVisible = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"logOut()\">确 定</el-button>\n      </span>\n    </el-dialog>\n    <br />\n    <br />\n    <br />\n    <br />\n    <p class=\"header\">Real People, Fake Reviews</p>\n    <br />\n    <div>\n      <el-input\n          style=\"width:300px;\"\n          placeholder=\"Type something\"\n          v-model=\"search_key\"\n          @input=\"search_key=search_key.replace(/\\s+/, '')\"\n          >\n        <i slot=\"prefix\" class=\"el-input__icon el-icon-search\"></i>\n      </el-input>\n      <el-button type=\"box\" icon=\"el-icon-search\"  @click=\"search()\">Search</el-button>\n    </div>\n\n    <br />\n    <br />\n    <br />\n    <br />\n    <br />\n    <br />\n    <br />\n    \n    <el-row>\n      <el-col :span=\"8\" v-for=\"(item,index) in items\" :key=\"index\" v-model=\"items[index]\" :offset=\"2\">\n          <el-card :body-style=\"{ padding: '5px' }\">\n            <img :src=\"item.fimg\" class=\"image\" height=\"325\" @click=\"showInfo(item.fid)\">\n            <div style=\"padding: 14px;\">\n              <span>{{item.fname}}</span><br/>\n<!--              <span>Food id is {{item.fid}}!</span>-->\n              <div class=\"bottom clearfix\">\n<!--                <time class=\"time\">{{ currentDate }}</time>-->\n              </div>\n            </div>\n          </el-card>\n          <br />\n      </el-col>\n    </el-row>\n\n    <br />\n    <br />\n  </div>\n</template>\n\n<style scoped>\nbutton{\n  box-shadow: inset 0 0 0 2px #212931;\n}\n.time {\nfont-size: 13px;\ncolor: #999;\n}\n\n.bottom {\n  margin-top: 13px;\n  line-height: 12px;\n}\n\n.button {\n  padding: 0;\n  float: right;\n}\n\n.image {\n  width: 100%;\n  display: block;\n  cursor: pointer;\n}\n\n.clearfix:before,\n.clearfix:after {\n  display: table;\n  content: \"\";\n}\n\n.clearfix:after {\n  clear: both\n}\n\n.header {\n  font: italic 3em Arial, sans-serif;\n  color: aliceblue;\n  font-weight: bold;\n  letter-spacing: 2.5pt;\n}\n\n\n</style>\n\n<script>\nimport $getData from '../api/getData'\nimport $logOut_func from '../common/logOut'\nexport default {\n  name: 'home',\n  data() {\n    return {\n      counter: 0,\n      items: [],\n      search_key:\"\",\n      currentDate: new Date(),\n      dialogVisible: false,\n      usr_bar:\"\"\n    }\n  },\n  methods: {\n    add() {\n      this.counter += 1\n    },\n    Home() {\n      this.$router_func.toHome(this)\n    },\n    User() {\n      this.$router_func.toUser(this)\n    },\n    Upload() {\n      this.$router_func.toUpload(this)\n    },\n    changePwd() {\n      this.$router_func.toChangePwd(this)\n    },\n    search() {\n      if (this.search_key == \"\") {\n        $getData.getList(this)\n      } else {\n        $getData.searchData(this, this.search_key)\n      }\n    },\n    Register() {\n      this.$router_func.toRegister(this)\n    },\n    logOut() {\n      this.dialogVisible = false\n      $logOut_func.logOut(this)\n      location.reload()\n    },\n    preLogOut() {\n      console.log(window.sessionStorage.getItem('login'))\n      if(!window.sessionStorage.getItem('login')) {\n        this.$message.error(\"您还未登录\")\n      }\n      else {\n        this.dialogVisible = true\n      }\n    },\n    showInfo(fid) {\n      console.log(\"show info fid \" + fid)\n      this.$router_func.toInfo(this, fid)\n    }\n  },\n  created: function () {\n    if(window.sessionStorage.getItem('login') == 'true') {\n      this.usr_bar = \"User Home\"\n    }\n    else {\n      this.usr_bar = \"Log In\"\n    }\n    $getData.getList(this)\n  }\n}\n</script>\n"]}]}