 }\n  else if (isLogin == 'loginPage') {\n    // 避免重复登录\n    if (window.sessionStorage.getItem('login') == 'true') {\n      alert('您已登录，请勿重复登陆')\n      next('/Home')\n    } else {\n      next()\n    }\n  }\n  else {\n    next()\n  }\n})\n\nnew Vue({\n  render: h => h(App),\n  router\n}).$mount('#app')\n"]}]}